  "private": false,
  "version": "1.0.0",
  "description": "an example of how to create a node website that uses a node package also written by you",
  "main": "index.js",
  "scripts": {
    "pack": "cls&&webpack --config=webpack.config.js",
    "pack-prod": "set NODE_ENV=production&&cls&&webpack --config=webpack.config.js",
    "start": "set NODE_ENV=hot&&npm run start-hot",
    "start-refresh": "webpack-dev-server --no-inline --no-hot --open --progress --colors --config webpack.config.js",
    "start-hot": "webpack-dev-server --inline --hot --open --progress --colors --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
